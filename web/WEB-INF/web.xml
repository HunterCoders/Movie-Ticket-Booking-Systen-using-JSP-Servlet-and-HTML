            <max-file-size>1048576</max-file-size>
            <max-request-size>2097152</max-request-size>
            <file-size-threshold>524288</file-size-threshold>
        </multipart-config>
    </servlet>
    <servlet>
        <servlet-name>AssignMovie</servlet-name>
        <servlet-class>AssignMovie</servlet-class>
    </servlet>
