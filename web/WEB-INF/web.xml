        <servlet-name>DeleteReco</servlet-name>
        <servlet-class>DeleteReco</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteMov</servlet-name>
        <servlet-class>DeleteMov</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImgTestServlet</servlet-name>
        <servlet-class>ImgTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadToDb</servlet-name>
        <servlet-class>UploadToDb</servlet-class>
